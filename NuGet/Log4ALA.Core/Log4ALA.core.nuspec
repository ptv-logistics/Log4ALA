<?xml version="1.0"?>
<package>
  <metadata minClientVersion="3.3.0">
    <id>Log4ALA.Core</id>
    <title>Log4ALA</title>
    <version>2.9.0</version>
    <authors>mob</authors>
    <owners>mob</owners>
    <projectUrl>https://ptv-logistics.github.io/Log4ALA/</projectUrl>
    <licenseUrl>https://github.com/ptv-logistics/Log4ALA/blob/master/LICENSE</licenseUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Log4Net appender fo Azure Log Analytics (ALA)... sending data to Azure Log Analytics. The data will also be logged/sent asynchronously for high performance and to avoid blocking the caller thread.</description>
    <copyright>Copyright Â© 2025 PTV Logistics</copyright>
    <releaseNotes>Add disableNumberTypeConvertion (default is false) to disable automatic type convertion of number values so it doesn't matter whether the value contains only numeric chars or alphanumeric chars to use the same key/column for both values this results in the column type string.</releaseNotes>
    <dependencies>
      <group targetFramework=".netstandard2.0">
        <dependency id="log4net" version="2.0.10" />
        <dependency id="Newtonsoft.Json" version="13.0.1" />
        <dependency id="Microsoft.Extensions.Configuration" version="2.0.0" />
        <dependency id="Microsoft.Extensions.Configuration.Abstractions" version="2.0.0" />
        <dependency id="Microsoft.Extensions.Configuration.Binder" version="2.0.0" />
        <dependency id="Microsoft.Extensions.Configuration.EnvironmentVariables" version="2.0.0" />
        <dependency id="Microsoft.Extensions.Configuration.FileExtensions" version="2.0.0" />
        <dependency id="Microsoft.Extensions.Configuration.Json" version="2.0.0" />
        <dependency id="Microsoft.Extensions.Primitives" version="2.0.0" />
        <dependency id="System.Net.Http" version="4.3.4" />
      </group>
    </dependencies>
	<!-- https://docs.microsoft.com/de-de/nuget/reference/nuspec#including-content-files 
    <contentFiles>
	  <files include="**/internalLog4net.config" buildAction="Content" flatten="true" copyToOutput="true" />
    </contentFiles>
	-->
  </metadata>
</package>
