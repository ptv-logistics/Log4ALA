<?xml version="1.0"?>
<package>
  <metadata minClientVersion="3.3.0">
    <id>Log4ALA</id>
    <title>Log4ALA</title>
    <version>2.9.1</version>
    <authors>mob</authors>
    <owners>mob</owners>
    <projectUrl>https://github.com/ptv-logistics/Log4ALA/</projectUrl>
    <licenseUrl>https://github.com/ptv-logistics/Log4ALA/blob/master/LICENSE</licenseUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Log4Net appender fo Azure Log Analytics (ALA)... sending data to Azure Log Analytics. The data will also be logged/sent asynchronously for high performance and to avoid blocking the caller thread.</description>
